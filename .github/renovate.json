{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "All minor and patch updates",
      "automergeType": "branch-push",
      "prCreation": "not-pending",
      "automerge": false
    }
  ]
}
